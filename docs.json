{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "TickDB Docs",
  "colors": {
    "primary": "#0D9373",
    "light": "#07C983",
    "dark": "#0D9373"
  },
  "favicon": "/logo.png",
  "api": {
    "mdx": {
      "server": "https://api.tickdb.ai",
      "auth": {
        "method": "key",
        "name": "X-API-Key"
      }
    }
  },
  "navigation": {
    "tabs": [
      {
        "tab": "Documentation",
        "groups": [
          {
            "group": "Introduction",
            "pages": [
              "index"
            ]
          },
          {
            "group": "Getting Started",
            "pages": [
              "quick-start",
              "authentication"
            ]
          },
          {
            "group": "REST API",
            "pages": [
              "rest/api_symbols",
              "rest/api_ticker",
              "rest/api_kline",
              "rest/api_depth",
              "rest/api_trades"
            ]
          },
          {
            "group": "WebSocket Docs",
            "pages": [
              "websocket/websocket_overview",
              "websocket/websocket_quickstart",
              "websocket/websocket_subscribe",
              "websocket/websocket_message_schema"
            ],
            "asyncapi": {
              "source": "/asyncapi.json",
              "directory": "websocket/websocket_playground"
            }
          },
          {
            "group": "Reference",
            "pages": [
              "data-specification",
              "errors"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Dashboard",
          "href": "https://tickdb.ai",
          "icon": "chart-line"
        },
        {
          "anchor": "Support",
          "href": "mailto:support@tickdb.ai",
          "icon": "envelope"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo.png",
    "dark": "/logo.png",
    "href": "https://tickdb.ai"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:support@tickdb.ai"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://tickdb.ai"
    }
  }
}